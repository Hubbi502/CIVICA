{
  "expo": {
    "name": "CIVICA",
    "slug": "CIVICA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/IMG-20260112-WA0039.png",
    "scheme": "civica",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/IMG-20260112-WA0039.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hubbielfairuz.CIVICA"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/IMG-20260112-WA0039.png",
          "imageWidth": 280,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/IMG-20260112-WA0039.png",
            "backgroundColor": "#1a1a2e"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c2d5d3d5-baaa-4161-bf6d-89775c089a90"
      }
    }
  }
}